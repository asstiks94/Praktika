import math

v0 = float(input("Початкова швидкість (м/с): "))
angle = float(input("Кут (градуси): "))
g = 9.81

rad = math.radians(angle)
v_y = v0 * math.sin(rad)
t_total = 2 * v_y / g
h_max = (v_y ** 2) / (2 * g)

print(f"Макс. висота: {h_max:.2f} м. Час польоту: {t_total:.2f} с.")

for t in range(int(t_total) + 1):
    h = v_y * t - (g * t**2) / 2
    print(f"Час {t}с: Висота {max(0, h):.2f} м")
